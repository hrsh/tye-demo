{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "default-sqlserver": "Data Source:.;Initial Catalog=<>;Integrated Security=true",
    "default-sqlite": "Data Source=<>.db"
  },
  "spring": {
    "application": {
      "name": "ServiceDiscoveryClient1"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "port": "8001",
      "ipAddress": "localhost",
      "preferIpAddress": true
    }
  }
}
